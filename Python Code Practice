# list problem
my_list = [10, 8, 6, 4, 2]
new_list = my_list[1:3]
print(new_list)

# list exercise 1
list_1 = ["A", "B", "C"]
list_2 = list_1
list_3 = list_2

del list_1[0]
del list_2[0]

print(list_3)

# for finding prime number
def is_prime(num):

    if num%2 == 0:
        return(str(num) + " is not a prime number")
    
    if num > 1:
        if num%2 == 1:
            return(str(num)+" is a prime number")

for i in range(1, 20):
    num = i+1
    print(is_prime(num))
print()

